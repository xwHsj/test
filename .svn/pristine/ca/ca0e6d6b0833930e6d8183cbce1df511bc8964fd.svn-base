/*hesj添加首页部分 */
.energyBox{overflow: hidden;}
.chart-box{padding: 20px;}
.chart-box .cb-header{}
.chart-box .cb-header .cb-title{color: #666;font-size: 12px;}
.chart-box .cb-header .cb-title-right{float:right;}
.chart-box .cb-header .cb-cut-date{width:91px;height:22px;line-height:22px;border:1px solid #cacccf;border-radius:2px;font-size:12px;font-family:SimHei;background-color:#ffffff;text-align:center;display: inline-block;box-sizing: border-box;}
.chart-box .cb-header .cb-cut-date a{width:8px;height:8px;margin:0 4px;display: inline-block;cursor: pointer;}
.chart-box .cb-header .cb-cut-date img{vertical-align: 0;}
.chart-box .cb-legend-blue{width:8px;height:4px;display: inline-block;background-color: #4e9cdd;vertical-align: middle;}
.chart-box .cb-legend-gray{width:8px;height:4px;display: inline-block;background-color: #c2ccd3;vertical-align: middle;}
.chart-box .cb-header .cb-title-right label{font-size: 12px;color: #666;}
.chart{width: 100%;height: 220px;margin-top:10px;}
.button-group a{padding:2px 15px;color: #666;font-size:12px;background-color: #ebecee;border-top-width:1px;border-bottom-width:1px;border-style: solid;border-color: #cccccc;display:inline-block;}
.button-group a.button-group-act{color:#ffffff;background-color: #7b8695;}
.button-group .bg-left{border-left-width:1px;border-radius: 2px 0 0 2px;}
.button-group .bg-right{border-left:1px #ffffff;border-right-width:1px;border-radius: 0 2px 2px 0;}
.cost-chart{width:60%;background: url(../img/icons/icon003.png) no-repeat right center;float: left;}
.cost-list{width:40%;height:313px;background-color: #FFFFFF;float: left;}
.cost-list-box{margin-left:19px;padding:10px 0 9px 43px;color:#666;border-bottom: 1px solid #eee;background-position:left center;background-repeat:no-repeat;}
.cost-list-name{font-size:12px;}
.cost-list-num{font-size:14px;}
.cost-list-proportion{margin-left:20px;font-size:11px;color:#bbbbbb;}
.cost-upper{}
.cost-list-hot .cost-list-num, .cost-list-hot .cost-list-proportion{color:#d4513b;}
.cost-list-nyf{background-image: url(../img/index/costicon01.png);}
.cost-list-sbf{background-image: url(../img/index/costicon02.png);}
.cost-list-rgf{background-image: url(../img/index/costicon03.png);}
.cost-list-glf{background-image: url(../img/index/costicon04.png);}
.cost-list-qtf{background-image: url(../img/index/costicon05.png);}

.emission-chart{width:60%;height:313px;background: url(../img/icons/icon003.png) no-repeat right center;position:relative;float: left;}
.wave{width:226px;height:226px;top:50%;left:50%;margin-top:-113px;margin-left:-113px;overflow:hidden;border-radius:50%;background:url(../img/index/bag.png) no-repeat;position:absolute;text-align:center;display:table-cell;vertical-align:middle;z-index:1}
.wave-main{position:absolute;z-index:3;width:226px;top:-20px}
.wave b{display:block;font-size:44px;font-weight:normal;text-align:center;margin-top:100px;color:#d4513b;}
.wave p{line-height:15px;margin:0;font-size: 12px;}
.wave .wave-name{color: #666;}
.wave .wave-proportion{color: #bbbbbb;}
.wave-hot b, .wave-hot .wave-proportion{color: #d4513b;}
.wave canvas{position:absolute;left:0;top:0;z-index:2;}

.emission-list{width:40%;height:313px;background-color: #FFFFFF;float: left;}
.emission-list-box{margin-left:19px;padding:15px 0 14px;color:#666;border-bottom: 1px solid #eee;}
.emission-list-rect{width:8px;height:8px;background-color:#8394aa;border-radius:2px;display: inline-block;}
.emission-list-name{font-size:12px;}
.emission-list-num{margin-left:15px;font-size:14px;}
.emission-list-proportion{margin-left:15px;font-size:11px;color:#bbbbbb;}
.emission-list-hot .emission-list-list-num, .emission-list-hot .emission-list-proportion{color:#d4513b;}
.emission-list-hot .emission-list-rect{background-color: #d4513b;}
